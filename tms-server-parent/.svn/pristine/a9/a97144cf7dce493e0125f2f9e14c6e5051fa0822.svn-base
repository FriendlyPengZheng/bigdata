.main .content .content-header { margin-bottom : 10px; height : 85px; border-radius : 5px; border : 1px solid #B6BDC5; box-shadow : 1px 1px 3px #8F949A; background : -webkit-linear-gradient(bottom, #F2F4F5, #FFFFFF); background-color: #EBF0F3; background: -o-linear-gradient(bottom, #F2F4F5, #ffffff); background: -moz-linear-gradient(bottom, #F2F4F5, #FFFFFF); background: -ms-linear-gradient(bottom, #F2F4F5, #FFFFFF); background: -webkit-gradient(linear,0 100%,0 0,from(#F2F4F5),to(#FFFFFF)); }
.content-header .view-data { position : absolute; top : 0; left : 0; color : #7A889B; font-size : 12px; }
.content-header .view-data li { float : left; text-align : right; margin-left : 10px; padding : 5px 10px 0 55px; min-height : 90px;  *height: 90px; }
.content-header .view-data li.view-data1 { background : url( ../../../../image/data/big_icon.png ) no-repeat 0 -276px;  }
.content-header .view-data li.view-data2 { background : url( ../../../../image/data/big_icon.png ) no-repeat 0 -480px;  }
.content-header .view-data li.view-data3 { background : url( ../../../../image/data/big_icon.png ) no-repeat 0 20px;  }
.content-header .view-data li .view-txt { margin : 10px 0 5px 0; display : inline-block; *display : inline; zoom : 1; }
.content-header .view-data li strong { display : block; }
.content-header .view-data li strong.imp { font-size : 18px; color : #465160; }
.content-header .view-data li strong.up { color : #F74B4E; }
.content-header .view-data li strong.down { color : #5BA54F; }

.progress-wrap { margin: 10px 10px 0 0; text-align: right; height: 20px; line-height: 20px; }
.progress-title { vertical-align: top; color: #7A7A7A; height: 20px; display: inline-block; *display: inline; zoom: 1; }
.progress-text { color: #6EC560; font-size: 14px; font-weight: bold; height: 20px; line-height: 20px; display: inline-block; *display: inline; zoom: 1; }
.progress-text.warning { color: #FD7577; }
