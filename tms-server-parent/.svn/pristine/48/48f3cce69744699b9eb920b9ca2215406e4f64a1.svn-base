<mapper namespace="com.taomee.tms.mgr.dao.LogInfoDao">
	<resultMap id="LogInfoMap" type="com.taomee.tms.mgr.entity.LogInfo">
		<id column="log_id" property="logId" jdbcType="INTEGER" />
		<result column="log_name" property="logName" jdbcType="VARCHAR" />
		<result column="type" property="type" jdbcType="INTEGER" />
		<result column="status" property="status" jdbcType="INTEGER" />
		<result column="add_time" property="addTime" jdbcType="TIMESTAMP"
			javaType="java.sql.Timestamp" />
	</resultMap>

	<sql id="LogInfoColumnList">
		log_id, log_name, type, status
	</sql>

	<insert id="insertLogInfo" parameterType="com.taomee.tms.mgr.entity.LogInfo"
		useGeneratedKeys="true" keyProperty="logId">
		insert into t_log_info (log_name, type, status)
		values (#{logName,jdbcType=VARCHAR}, #{type,jdbcType=INTEGER}, status)
	</insert>

	<select id="getLogInfoByLogId" resultMap="LogInfoMap"
		parameterType="java.lang.Integer">
		select
		<include refid="LogInfoColumnList" />
		from t_log_info
		where log_id = #{logId,jdbcType=INTEGER}
	</select>
	
	<select id="getLogInfoBylogName" resultMap="LogInfoMap"
		parameterType="java.lang.String">
		select
		<include refid="LogInfoColumnList" />
		from t_log_info
		where log_name = #{logName,jdbcType=VARCHAR}
	</select>

	<select id="getLogInfos" resultMap="LogInfoMap">
		select
		<include refid="LogInfoColumnList" />
		from t_log_info order by log_id ASC
	</select>
	
	<select id="getLogInfosByStatus" resultMap="LogInfoMap" parameterType="java.lang.Integer">
		select
		<include refid="LogInfoColumnList" />
		from t_log_info  where status=#{status,jdbcType=INTEGER} order by log_id ASC
	</select>

	<update id="updateLogInfo" parameterType="com.taomee.tms.mgr.entity.LogInfo">
		update
		t_log_info
		set
		log_name=#{logName}
		where log_id = #{logId,jdbcType=INTEGER}
	</update>

	<update id="updateLogInfoByStatus" parameterType="com.taomee.tms.mgr.entity.LogInfo">
		update
		t_log_info
		set
		status=#{status}
		where log_id = #{logId,jdbcType=INTEGER}
	</update>

	<delete id="deleteLogInfo" parameterType="com.taomee.tms.mgr.entity.LogInfo">
		delete from
		t_log_info
		where log_id = #{logId,jdbcType=INTEGER}
	</delete>

