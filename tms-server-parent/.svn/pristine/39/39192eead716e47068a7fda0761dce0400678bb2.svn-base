public class LogSplitFunction extends BaseFunction {
	private static final long serialVersionUID = 5097236152288190818L;
	private static final Logger LOG = LoggerFactory.getLogger(LogSplitFunction.class);
	
	private RealtimeLogAnalyser realtimelogAnalyser = new RealtimeLogAnalyser();
	private Configer config;
	
	public LogSplitFunction(){
		
	}
	
	public LogSplitFunction(Configer config){
		this.config = config;
	}

	@Override
	public void prepare(@SuppressWarnings("rawtypes") Map conf, TridentOperationContext context) {
		LOG.info("LogSplitFunction prepare() method starting...");
		
    	ApplicationConfig application = new ApplicationConfig();
    	application.setName(config.getDubboServiceName());
    	
    	RegistryConfig registry = new RegistryConfig();
    	registry.setProtocol(config.getDubboRegistryProtocol());
    	registry.setAddress(config.getDubboRegistryAdress());
    	 
    	ReferenceConfig<LogMgrService> reference = new ReferenceConfig<LogMgrService>();
    	reference.setApplication(application);
    	reference.setRegistry(registry);
    	reference.setInterface(LogMgrService.class);
    	 
    	LogMgrService logMgrService = reference.get();

		try {
			if (!realtimelogAnalyser.Init(logMgrService.getSchemaInfosByLogType(0),logMgrService.getAllServerInfos(Integer.valueOf(0)))) {
				LOG.error("LogSplitFunction prepare, logParser Init failed");
				return;
			}
		} catch (Exception e) {
			LOG.error("LogSplitFunction prepare, logParser Init failed while init realtimelogAnalyser, " + e.getMessage());
			e.printStackTrace();
		}
		
		LOG.info("LogSplitFunction prepare() method finished.");
		
	}

	public void execute(TridentTuple tuple, TridentCollector collector) {
		if (realtimelogAnalyser.SetLog(tuple.getString(0))) {
			// 处理内存中schema的增删改
			if (realtimelogAnalyser.IsDealingSpecailLog()) {
				if (!realtimelogAnalyser.UpdateConfig()) {
					LOG.error("LogSplitFunction execute, UpdateConfig failed");
					return;
				}
			} else {
				List<RealtimePlainLogItem> logItems = realtimelogAnalyser.GetAllRealTimeLogItems();
				if (logItems != null) {
					for (RealtimePlainLogItem item : logItems) {
						LOG.info("emit log item is " + item.toString());
						List<Object> values = new ArrayList<Object>();
						values.add(item.getStrOp());
						values.add(item.getSchemaId());
						values.add(item.getServerId());
						values.add(item.getCascadeValue());
						values.add(item.getOpValues());
						values.add(item.getDateTime());
						collector.emit(values);
					}
				}
			}
		}
	}
	
