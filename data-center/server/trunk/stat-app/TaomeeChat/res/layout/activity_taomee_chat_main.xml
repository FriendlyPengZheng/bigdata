<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools" android:id="@+id/container"
	android:layout_width="match_parent" android:layout_height="match_parent"
	tools:context="main_taomeechat.TaomeeChatMainActivity" tools:ignore="MergeRootFrame">

	<LinearLayout android:layout_width="match_parent"
		android:layout_height="match_parent" android:orientation="vertical">

		<LinearLayout android:id="@+id/msg_layout"
			android:layout_width="match_parent" android:layout_height="0dp"
			android:layout_weight="0.74" android:orientation="horizontal">
		</LinearLayout>


		<LinearLayout android:id="@+id/operator_bar_layout"
			android:layout_width="fill_parent" android:layout_height="45dp"
			android:orientation="horizontal">
		</LinearLayout>

	</LinearLayout>

</FrameLayout>