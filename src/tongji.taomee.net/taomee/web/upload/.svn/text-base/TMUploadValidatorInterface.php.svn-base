<?php
interface TMUploadValidatorInterface
{
    /**
     * Validate file
     *
     * This method is responsible for validating an `TMUploadFileInfoInterface` instance.
     * If validation fails, an exception should be thrown.
     *
     * @param  TMUploadFileInfoInterface  $fileInfo
     * @throws TMUploadValidatorException If validation fails
     */
    public function validate(TMUploadFileInfoInterface $fileInfo);
}
