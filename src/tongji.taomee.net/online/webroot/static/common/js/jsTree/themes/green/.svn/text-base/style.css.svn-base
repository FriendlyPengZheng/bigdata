/*
 * jsTree green theme 1.0
 * Supported features: dots/no-dots, icons/no-icons, focused, loading
 * Supported plugins: ui (hovered, clicked), checkbox, contextmenu, search
 */

.jstree-green a { color: #333; border-radius: 5px; -webkit-border-radius: 5px; -moz-border-radius: 5px; }
.jstree-green ins { background-image:url("toolbar.png"); background-repeat:no-repeat; background-color:transparent; }
.jstree-green a .jstree-icon { background-image:url("toolbar.png"); background-repeat:no-repeat; background-color:transparent; }
.jstree-green .jstree-open ins { background-position: 0 -11px; }
.jstree-green .jstree-closed ins { background-position: 0px 5px; }
.jstree-green .jstree-leaf ins { background-position: 1000px 0; }

.jstree-green .jstree-clicked { background-color: #FBBB67; }

.jstree-green a .jstree-icon { background-position: 0 -57px; }
.jstree-green a.jstree-loading .jstree-icon { background:url("throbber.gif") center center no-repeat !important; }

.jstree-green.jstree-focused { background: transparent; }

.jstree-green .jstree-no-dots li, 
.jstree-green .jstree-no-dots .jstree-leaf > ins { background: transparent; }
.jstree-green .jstree-no-dots .jstree-open > ins { background-position: -18px 0; }
.jstree-green .jstree-no-dots .jstree-closed > ins { background-position: 0 0; }

.jstree-green .jstree-no-icons a .jstree-icon { display:none; }

.jstree-green .jstree-search { font-style:italic; }

.jstree-green .jstree-no-icons .jstree-checkbox { display:inline-block; }
.jstree-green .jstree-no-checkboxes .jstree-checkbox { display:none !important; }
.jstree-green .jstree-checked > a > .jstree-checkbox { background-position:-38px -19px; }
.jstree-green .jstree-unchecked > a > .jstree-checkbox { background-position:-2px -19px; }
.jstree-green .jstree-undetermined > a > .jstree-checkbox { background-position:-20px -19px; }
.jstree-green .jstree-checked > a > .jstree-checkbox:hover { background-position:-38px -37px; }
.jstree-green .jstree-unchecked > a > .jstree-checkbox:hover { background-position:-2px -37px; }
.jstree-green .jstree-undetermined > a > .jstree-checkbox:hover { background-position:-20px -37px; }

#vakata-dragged.jstree-green ins { background:transparent !important; }
#vakata-dragged.jstree-green .jstree-ok { background:url("d.png") -2px -53px no-repeat !important; }
#vakata-dragged.jstree-green .jstree-invalid { background:url("d.png") -18px -53px no-repeat !important; }
#jstree-marker.jstree-green { background:url("d.png") -41px -57px no-repeat !important; text-indent:-100px; }

.jstree-green a.jstree-search { color:aqua; }
.jstree-green .jstree-locked a { color:silver; cursor:pointer; }

#vakata-contextmenu.jstree-green-context, 
#vakata-contextmenu.jstree-green-context li ul { background:#f0f0f0; border:1px solid #979797; -moz-box-shadow: 1px 1px 2px #999; -webkit-box-shadow: 1px 1px 2px #999; box-shadow: 1px 1px 2px #999; }
#vakata-contextmenu.jstree-green-context li { }
#vakata-contextmenu.jstree-green-context a { color:black; }
#vakata-contextmenu.jstree-green-context a:hover, 
#vakata-contextmenu.jstree-green-context .vakata-hover > a { padding:0 5px; background:#e8eff7; border:1px solid #aecff7; color:black; -moz-border-radius:2px; -webkit-border-radius:2px; border-radius:2px; }
#vakata-contextmenu.jstree-green-context li.jstree-contextmenu-disabled a, 
#vakata-contextmenu.jstree-green-context li.jstree-contextmenu-disabled a:hover { color:silver; background:transparent; border:0; padding:1px 4px; }
#vakata-contextmenu.jstree-green-context li.vakata-separator { background:white; border-top:1px solid #e0e0e0; margin:0; }
#vakata-contextmenu.jstree-green-context li ul { margin-left:-4px; }

/* IE6 BEGIN */
.jstree-green li, 
.jstree-green ins,
#vakata-dragged.jstree-green .jstree-invalid, 
#vakata-dragged.jstree-green .jstree-ok, 
#jstree-marker.jstree-green { _background-image:url("d.gif"); }
.jstree-green .jstree-open ins { background-position:0px -11px; }
.jstree-green .jstree-closed ins { background-position:0px 5px; }
.jstree-green .jstree-leaf ins { background-position:1000px 0; }
.jstree-green a ins.jstree-icon { background-position:0px -57px; }
.jstree-green a ins.root { background-position:0px -32px; }
#vakata-contextmenu.jstree-green-context ins { display:none; }
#vakata-contextmenu.jstree-green-context li { zoom:1; }
.jstree-green .jstree-undetermined a .jstree-checkbox { background-position:-20px -19px; }
.jstree-green .jstree-checked a .jstree-checkbox { background-position:-38px -19px; }
.jstree-green .jstree-unchecked a .jstree-checkbox { background-position:-2px -19px; }
/* IE6 END */

.jstree-green .jstree-open a ins { background-position:0px -129px; }
.jstree-green .jstree-closed a ins { background-position:0px -57px; }
.jstree-green .jstree-leaf a ins { background-position: 0px -541px; }
