<div class="wrapper">
    {if isset($response.second_bar) && !empty($response.second_bar)}
    <ul class="main-sub-nav">
        {foreach $response.second_bar as $second_bar}
             <li
             {if isset($second_bar.current) && $second_bar.current}
                class="cur"
             {/if}
             data-key="{$second_bar.key}"><a href="{$second_bar.url}">{$second_bar.name}</a></li>
        {/foreach}
        {if isset($response.admin_auth)}
        <li class="sub-nav-li posr stat-button" title="模板管理">
        <a href="index.php?r=admin/manage/displayPage&page_url={$response.param.r}&navi_key={$response.current.pageKey}&module_name={$response.current.pageTitle}" class="bar-manage"><i class="iconfont icon-config"></i>{TM::t('tongji', '模板管理')}</a>
        </li>
        {/if}
    </ul>
    {/if}
</div>
