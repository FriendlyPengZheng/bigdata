/* search */
.search-wrapper { font-size: 12px; position: absolute; top: 0; right: 0; height: 24px; line-height: 24px; color: #635E59; }
.search-wrapper .search-ipt { width: 160px; color: #635E59; font-size: 12px;  padding: 2px; margin-left: 5px; height: 18px; line-height: 18px; border: 1px solid #BBB; border-radius: 4px; -moz-border-radius: 4px; -webkit-border-radius: 4px; }
.search-wrapper .search-ipt:focus { box-shadow: 0px 1px 3px 0 #797979; -moz-box-shadow: 0px 1px 3px 0 #797979; -webkit-border-shadow: 0px 1px 3px 0 #797979; }
