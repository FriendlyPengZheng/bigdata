* { margin: 0; padding: 0; }
ul { list-style: none; padding: 0; }
img { border: 0; }
table { border-collapse: collapse; border: 0; }

/************************************* Common **************************************/

/* font */
.f12  { font-size: 12px; }
.f12b { font-size: 12px; font-weight: bold; }
.f14  { font-size: 14px; }
.f14b { font-size: 14px; font-weight: bold; }
.f16  { font-size: 16px; }
.f16b { font-size: 16px; font-weight: bold; }
.f18  { font-size: 18px; }
.f18b { font-size: 18px; font-weight: bold; }
.fb   { font-weight: bold; }
.p12n { font-size: 12px; line-height: 18px; vertical-align: middle; }
.p12d { font-size: 12px; line-height: 24px; vertical-align: middle; }

/* text alignment */
.tal  { text-align: left; }
.tac  { text-align: center; }
.tar  { text-align: right; }

/* float */
.fl { float: left; }
.fr { float: right; }

/* position */
.abs { position: absolute; }
.rel { position: relative; }

/* z index */
.z1    { z-index: 1; }
