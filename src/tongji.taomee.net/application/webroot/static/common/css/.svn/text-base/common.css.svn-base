/* cursor */
.cp { cursor: pointer; }
.cm { cursor: move; }
.cn { cursor: default; }

/* word break */
.wb { word-break: break-all; word-wrap: break-word; }

/**
* @brief float fixed
*/
.clearfix:after { clear: both; }
.clearfix:before, .clearfix:after { content: "."; display: block; height: 0; visibility: hidden; }
.clearfix { zoom: 1; }

/* special style */
.no-ie { display: none\9; }
.dp-inline{ display: inline; }
.dp-block{ display: block; }
.dp-ib { display: inline-block; zoom: 1; }


/* css3 styles */
.shadow  { box-shadow:5px 5px 20px #CCCCCC; -moz-box-shadow:5px 5px 20px #CCCCCC; -webkit-box-shadow:5px 5px 20px #CCCCCC; }

.radius3  { border-radius: 3px;  -moz-border-radius: 3px;  }
.radius5  { border-radius: 5px;  -moz-border-radius: 5px;  }
.radius8  { border-radius: 8px;  -moz-border-radius: 8px;  }
.radius10 { border-radius: 10px; -moz-border-radius: 10px; }
.radius20 { border-radius: 20px; -moz-border-radius: 20px; }

.bottom-radius3  { border-bottom-left-radius: 3px; border-bottom-right-radius: 3px; -moz-border-radius-bottomleft: 3px;  -moz-border-radius-bottomright: 3px;}
.bottom-radius5  { border-bottom-left-radius: 5px; border-bottom-right-radius: 5px; -moz-border-radius-bottomleft: 5px;  -moz-border-radius-bottomright: 5px;  }
.bottom-radius8  { border-bottom-left-radius: 8px; border-bottom-right-radius: 8px; -moz-border-radius-bottomleft: 8px;  -moz-border-radius-bottomright: 8px;  }
.bottom-radius10 { border-bottom-left-radius: 10px; border-bottom-right-radius: 10px; -moz-border-radius-bottomleft: 10px;  -moz-border-radius-bottomright: 10px; }
.bottom-radius20 { border-bottom-left-radius: 20px; border-bottom-right-radius: 20px; -moz-border-radius-bottomleft: 20px;  -moz-border-radius-bottomright: 20px; }

/* html5 element */
header, footer, nav, section { display: block; }
