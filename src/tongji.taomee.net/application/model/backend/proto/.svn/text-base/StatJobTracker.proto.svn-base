message backend_proto_StatJobTracker
{
    required uint16 registerTotal = 1;
    required uint16 alarmTotal = 2;
    repeated backend_proto_StatJobTrackerInfo info = 3 [repeatType = uint16];
}
message backend_proto_StatJobTrackerInfo
{
    required uint32 ip = 1;
    required uint16 port = 2;
    required uint8 redFlag = 3;
    required uint8 forbiddenFlag = 4;
    required uint64 lastHeartBeatTime = 5;
    required int32  activeTaskTrackers = 6;
    required int32  blackListedTaskTrackers= 7;
    required int32  runningMapTasks = 8;
    required int32  maxMapTasks = 9;
    required int32  runningReduceTasks = 10;
    required int32  maxReduceTasks = 11;
    required int32  failed = 12;
    required int32  killed = 13;
    required int32  prep = 14;
    required int32  running = 15;
    required string storageInfo = 16 [lengthType = int32];
}


